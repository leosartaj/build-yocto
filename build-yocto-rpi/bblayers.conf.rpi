# POKY_BBLAYERS_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  /src/poky-warrior/meta \
  /src/poky-warrior/meta-poky \
  /src/poky-warrior/meta-yocto-bsp \
  /src/poky-warrior/meta-raspberrypi \
  "
